mod_bp4_null <- bpaft(formula = Surv(time, status) ~ 1, approach = "bayes", data = veteran2)
p3 <- ggplot() +
geom_point(aes(
x = veteran2$karno, residuals(mod_bp3_null),
color = factor(veteran2$status)
)) +
ylim(-4, 2) +
geom_smooth(aes(x = veteran2$karno, residuals(mod_bp3_null)),
